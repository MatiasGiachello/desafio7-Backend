<div class="formContainer">
  <div class="accessForm">
    <h2>Iniciar Sesión</h2>
    <form id="accessForm">
      <label for="email">Correo Electrónico</label>
      <input type="text" name="email" type="email" placeholder="example@gmail.com" />
      <label for="password">Contraseña</label>
      <input name="password" type="password" placeholder="********" />
      <input class="loginBtn" type="submit" value="Iniciar Sesion" />
    </form>
    <div class="socialLogin">
      <div class="orSection">
        <hr />
        <span>O</span>
        <hr />
      </div>
      <div class="socialBtnLogin">
        <a class="btnSocialLogin" href="/api/sessions/google"><img src="/img/icons/google.png" alt="" />
          Entrar con Google</a>
        <a class="btnSocialLogin" href="/api/sessions/github"><i class="fab fa-github" style="color: #0d0d0d;"></i>
          Entrar con GitHub</a>
      </div>
    </div>
    <p>¿No tienes una cuenta? <a href="/register">Regístrate</a></p>
  </div>
</div>

<script src="/js/login.js"></script>